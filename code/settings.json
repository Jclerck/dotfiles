{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "apple",
    "browserslist",
    "camelcase",
    "checkbox",
    "clerck",
    "dialog",
    "emoji",
    "fallback",
    "firefox",
    "flowtype",
    "header",
    "icon",
    "keyframes",
    "lcov",
    "localforage",
    "minmax",
    "nomodule",
    "placeholder",
    "polyfill",
    "popover",
    "precomposed",
    "promisify",
    "refactor",
    "setup",
    "shortcut",
    "snaphot",
    "spec",
    "touch",
    "unmount",
    "workflow"
  ],
  "debug.node.autoAttach": "on",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.cursorBlinking": "expand",
  "editor.foldingStrategy": "indentation",
  "editor.fontFamily": "Operator Mono SSm, Monaco, monospace, webicons",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "constant",
          "entity.name.type.js",
          "keyword.control.conditional.js",
          "keyword.control.export.js",
          "keyword.control.flow.js",
          "keyword.control.loop.js",
          "keyword.control.trycatch.js",
          "keyword.operator.new.js",
          "keyword.other.debugger.js",
          "meta.function-call.js",
          "meta.import.js",
          "meta.return.type.js",
          "meta.tag.attributes.js",
          "meta.type.declaration.js",
          "storage.modifier.async.js",
          "storage.modifier.js",
          "storage.type.class.js",
          "storage.type.function.js",
          "string.quoted.single.js",
          "string.regexp.js",
          "string.template.js",
          "support.function.dom.js",
          "support.function.promise.js",
          "support.type.primitive.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "meta.definition.function.js",
          "support.class.promise.js",
          "support.constant.json.js",
          "support.constant.math.js",
          "support.variable.dom.js",
          "support.variable.property.dom.js",
          "variable.language.this.js",
          "variable.other.object.js",
          "variable.other.object.property.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "token.debug-token",
          "token.error-token",
          "token.info-token",
          "token.warn-token"
        ],
        "settings": {
          "foreground": "#FF0000",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "eslint.alwaysShowStatus": true,
  "explorer.openEditors.visible": 0,
  "files.exclude": {
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.statusBar.format": "${author}, ${agoOrDate} â€¢ ${id}",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "npm.enableScriptExplorer": true,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "problems.decorations.enabled": true,
  "search.exclude": {
    "**/bower_components": false,
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules": false,
    "**/node_modules/**": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.closeWhenEmpty": true,
  "window.title": "${rootName}${separator}${activeEditorShort} ${dirty}",
  "workbench.colorCustomizations": {
    "statusBar.background": "#16825d",
    "statusBar.debuggingBackground": "#407886",
    "statusBar.noFolderBackground": "#7f007f"
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.statusBar.feedback.visible": false,
}
