{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "apple",
    "browserslist",
    "camelcase",
    "checkbox",
    "clerck",
    "dialog",
    "emoji",
    "fallback",
    "firefox",
    "header",
    "icon",
    "keyframes",
    "lcov",
    "localforage",
    "minmax",
    "nomodule",
    "placeholder",
    "polyfill",
    "popover",
    "precomposed",
    "promisify",
    "refactor",
    "setup",
    "shortcut",
    "snaphot",
    "sourcemap",
    "spec",
    "touch",
    "touchbar",
    "unmount",
    "workflow"
  ],
  "debug.node.autoAttach": "on",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.cursorBlinking": "expand",
  "editor.foldingStrategy": "indentation",
  "editor.fontFamily": "Operator Mono SSm, Monaco, monospace, webicons",
  "editor.fontSize": 16,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "constant",
          "entity.name.type.js",
          "keyword.control.conditional.js",
          "keyword.control.export.js",
          "keyword.control.flow.js",
          "keyword.control.loop.js",
          "keyword.control.trycatch.js",
          "keyword.operator.new.js",
          "keyword.other.debugger.js",
          "meta.function-call.js",
          "meta.import.js",
          "meta.return.type.js",
          "meta.tag.attributes.js",
          "meta.type.declaration.js",
          "storage.modifier.async.js",
          "storage.modifier.js",
          "storage.type.class.js",
          "storage.type.function.js",
          "string.quoted.single.js",
          "string.regexp.js",
          "string.template.js",
          "support.function.dom.js",
          "support.function.promise.js",
          "support.type.primitive.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "meta.definition.function.js",
          "support.class.promise.js",
          "support.constant.json.js",
          "support.constant.math.js",
          "support.variable.dom.js",
          "support.variable.property.dom.js",
          "variable.language.this.js",
          "variable.other.object.js",
          "variable.other.object.property.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "token.debug-token",
          "token.error-token",
          "token.info-token",
          "token.warn-token"
        ],
        "settings": {
          "foreground": "#FF0000",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "explorer.openEditors.visible": 0,
  "files.autoSaveDelay": 2500,
  "files.exclude": {
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "gitlens.mode.active": "zen",
  "npm.enableScriptExplorer": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "problems.decorations.enabled": true,
  "search.exclude": {
    "**/bower_components": false,
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules": false,
    "**/node_modules/**": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.closeWhenEmpty": true,
  "window.title": "${rootName}${separator}${activeEditorShort} ${dirty}",
  "workbench.colorCustomizations": {
    "statusBar.background": "#16825d",
    "statusBar.debuggingBackground": "#407886",
    "statusBar.noFolderBackground": "#7f007f"
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "files.autoSave": "afterDelay",
}
