{
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.cursorBlinking": "expand",
  "editor.fontFamily": "Operator Mono SSm, Monaco, monospace, webicons",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.formatOnSave": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "flow.enabled": false,
  "git.autofetch": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.codeLens.scopes": [],
  "gitlens.gitExplorer.enabled": false,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.keymap": "chorded",
  "gitlens.recentChanges.highlight.locations": [],
  "javascript.validate.enable": false,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "problems.decorations.enabled": true,
  "python.pythonPath": "python3",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 18,
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "inherit",
  "workbench.colorTheme": "Dracula",
  "workbench.editor.revealIfOpen": true
}
